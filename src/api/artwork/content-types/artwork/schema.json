{
  "kind": "collectionType",
  "collectionName": "artworks",
  "info": {
    "singularName": "artwork",
    "pluralName": "artworks",
    "displayName": "Artwork (Text und Bilder)",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titel": {
      "type": "string",
      "required": true
    },
    "Jahr": {
      "type": "biginteger",
      "required": true
    },
    "Media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Titelbild": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "Titel",
      "required": true
    },
    "Beschreibung": {
      "type": "richtext",
      "required": true
    },
    "reihenfolge": {
      "type": "biginteger",
      "required": true
    },
    "Begleittext": {
      "type": "richtext",
      "required": true
    },
    "includesImages": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "includesAudio": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "includesVideoLink": {
      "type": "boolean",
      "default": false
    },
    "videoLink": {
      "type": "string"
    }
  }
}
